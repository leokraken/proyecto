
@{
    ViewBag.Title = "Crear Consulta Masiva";
    Layout = "~/Views/Shared/_Admin.cshtml";
}

<ol class="breadcrumb">
    <li><a href="#">Consultas</a></li>
    <li class="active">Crear Consulta Masiva</li>
</ol>

<div class="paragraphs">
    <div class="row">
        <div class="span4">
            <div class="content-heading" style="margin-left:17px !important;">

                <h3 class="pull-left">Crear Consulta Masiva &nbsp &nbsp &nbsp &nbsp </h3>
                <img src="~/Content/images/agenda2.png" />
            </div>

        </div>
    </div>
</div>

<hr />

<div class="form-group">
    <label>Template &nbsp &nbsp &nbsp </label>

    @Html.ActionLink("Descargar", "DownloadTemplate", "Consulta")
</div>

@using (Html.BeginForm("Upload", "Consulta", FormMethod.Post, new { enctype = "multipart/form-data" }))
{
    
    
    <div class="form-group">
        <label>Archivo a Subir</label>
        <input type="file" name="UploadedFile">
         
    </div>
    <button type="submit" class="btn btn-default">Subir Archivo</button>
}

